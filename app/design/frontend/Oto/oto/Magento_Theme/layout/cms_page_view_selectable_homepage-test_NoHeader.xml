<!--
/**
 * Custom layout update handle CMS page Magento 2.3.4+.
 */
-->
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">

    <body>
      <attribute name="class" value="header-overlay"/>
<referenceBlock name="logo">
            <arguments>
                <argument name="logo_file" xsi:type="string">images/logo-white.png</argument>
                <argument name="logo_img_width" xsi:type="number">200</argument>
                <argument name="logo_img_height" xsi:type="number">60</argument>
                
            </arguments>
        </referenceBlock>
      

    </body>
</page>